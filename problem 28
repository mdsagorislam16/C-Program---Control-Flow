#include <stdio.h>
#include <math.h>

int main() {
    int n, temp, digits = 0, sum = 0;
    scanf("%d", &n);

    temp = n;

    for (int t = n; t > 0; t /= 10) {
        digits++;
    }

    for (temp = n; temp > 0; temp /= 10) {
        sum = sum + pow(temp % 10, digits);
    }

    if (sum == n) {
        printf("Armstrong\n");
    }
    else {
        printf("Not Armstrong\n");
    }

    return 0;
}
